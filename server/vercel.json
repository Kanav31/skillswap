{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/auth/(.*)",
            "dest": "/routes/main.js"
        },
        {
            "src": "/api/v1/profile/(.*)",
            "dest": "/routes/userProfile"
        },
        {
            "src": "/api/v1/matching/(.*)",
            "dest": "/routes/matchingAlgorithm"
        },
        {
            "src": "/api/v1/send/(.*)",
            "dest": "/routes/notification"
        },
        {
            "src": "/api/v1/chat-request/(.*)",
            "dest": "/routes/chatRequest"
        },
        {
            "src": "/api/v1/chat/(.*)",
            "dest": "/routes/chatService"
        },
        {
            "src": "/api/v1/message/(.*)",
            "dest": "/routes/messageRoute"
        },
        {
            "src": "/api/v1/tasklist-related/(.*)",
            "dest": "/routes/tasklistRoutes"
        },
        {
            "src": "/api/v1/Q&A/(.*)",
            "dest": "/routes/Q&ARoute"
        },
        {
            "src": "/controllers/(.*)",
            "dest": "/controllers/$1"
        }
    ]
}